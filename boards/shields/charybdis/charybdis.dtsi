#include <dt-bindings/zmk/matrix_transform.h>
#include <physical_layouts.dtsi>

/ {
    chosen {
        zmk,physical-layout = &charybdis_physical_layout;
    };
    kscan0: kscan {
        compatible = "zmk,kscan-gpio-matrix";
        wakeup-source;
        diode-direction = "col2row";
    }; 
    transform0: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        rows = <5>;
        columns = <16>;
        map = <
            RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)                                                                      RC(0,10) RC(0,11) RC(0,12) RC(0,13) RC(0,14) RC(0,15)
            RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5)                                                                      RC(1,10) RC(1,11) RC(1,12) RC(1,13) RC(1,14) RC(1,15)
            RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5)             RC(2,6) RC(2,7)            RC(2,8) RC(2,9)               RC(2,10) RC(2,11) RC(2,12) RC(2,13) RC(2,14) RC(2,15)
            RC(3,0) RC(3,1) RC(3,2) RC(3,3) RC(3,4) RC(3,5)                     RC(3,7)            RC(3,8)                       RC(3,10) RC(3,11) RC(3,12) RC(3,13) RC(3,14) RC(3,15)
                    RC(4,1) RC(4,2) RC(4,3) RC(4,4)             RC(4,5) RC(4,6) RC(4,7)            RC(4,8) RC(4,9) RC(4,10)               RC(4,11) RC(4,12) RC(4,13) RC(4,14)
        >;
    };
